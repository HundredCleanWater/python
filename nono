import requests
from bs4 import BeautifulSoup

headers = {'User-Agent' : 'Mozilla/5.0 (Windows NT 10.0; Win64; x64)AppleWebKit/537.36 (KHTML, like Gecko) Chrome/73.0.3683.86 Safari/537.36'}
data = requests.get('http://www.yes24.com/24/category/bestseller?CategoryNumber=001001003022003&sumgb=06',headers=headers)

soup = BeautifulSoup(data.text, 'html.parser')

title = soup.select_one('#category_layout > tbody > tr:nth-child(1) > td.goodsTxtInfo > p:nth-child(1) > a:nth-child(1)')

print(title)
